<!DOCTYPE html>
<html lang="en-US" dir="ltr">

<head>
    <% include ../../../partials/includes %>
    <link rel="stylesheet" href="/stylesheets/cadastro-investidor.css">
    <script type="text/javascript" src="/scripts/multiStepForm.js"></script>
</head>

<body data-spy="scroll" data-target=".onpage-navigation" data-offset="60">
    <main>
        <% include ../../../partials/headerInvestidor %>
        <div class="registration-page">
            <section class="features-section-1 relative image-bg-5 full-screen-js">
                <div class="container">
                    <div class="row inner-wrapper">
                        <div class="col-xs-12">
                            <div class="template-logo text-center">
                                <a class="logo center img-responsive" href="#"><img src="/img/logo-light.png" alt="Main Logo" /></a>
                            </div>
                            <div class="section-header margin-t-30 margin-b-30 text-center light-text">
                                <h4 class="section-heading text-uppercase letter-spacing-4">formulário de investidor</h4></div>
                        </div>
                        <div class="col-md-10 col-md-offset-1 col-sm-12">
                            <div class="row">
                                <div class="registration-form col-md-offset-3 col-md-6 col-sm-offset-2 col-sm-8 col-xs-12">
                                    <div class="registration-form-outer background-1">
                                        <% if(typeof errors !== 'undefined') { %>
                                        <% 		errors.forEach(function(error){ %>
                                              <div class="alert alert-danger"><%= error.msg %></div>
                                        <%		}); %>
                                        <% } %>
                                        <form class="single-form" method="post" action="/dashboard/cadastro-investidor/edit" enctype="multipart/form-data">
                                            <div class="form-section current">
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-user"></i></div>
                                                          <input class="form-control" name="name" type="text" placeholder="Nome completo" value="<%= investor.name %>"/>
                                                      </div>
                                                  </div>
                                              </div>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-envelope"></i></div>
                                                          <input class="form-control cpfMask" name="cpf" type="text" placeholder="CPF" maxlength="14" value="<%= investor.CPF %>"/>
                                                      </div>
                                                  </div>
                                              </div>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                                                          <input class="form-control dataMask" name="date" type="text" placeholder="Data de nascimento" maxlength="10" value="<%= investor.birthday %>"/>
                                                      </div>
                                                  </div>
                                              </div>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                                                          <input class="form-control telefoneMask" name="telephone" type="text" placeholder="Telefone" maxlength="14" value="<%= investor.phone %>"/>
                                                      </div>
                                                  </div>
                                              </div>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                                                          <input class="form-control celularMask" name="cellphone" type="text" placeholder="Celular" maxlength="15" value="<%= investor.cellphone %>"/>
                                                      </div>
                                                  </div>
                                              </div>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                                                          <input class="form-control moneyMask" name="renda" type="text" placeholder="Renda mensal declarada" value="<%= investor.renda %>" />
                                                      </div>
                                                  </div>
                                              </div>
                                            </div>
                                            <div class="form-section">
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-user"></i></div>
                                                          <input class="form-control cep" name="cep" type="text" placeholder="CEP" maxlength="8" value="<%= investor.cep %>" />
                                                      </div>
                                                  </div>
                                              </div>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-envelope"></i></div>
                                                          <input class="form-control logradouro" name="logradouro" type="text" placeholder="Logradouro" value="<%= investor.logradouro %>"/>
                                                      </div>
                                                  </div>
                                              </div>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                                                          <input class="form-control" name="number" type="text" placeholder="Número" value="<%= investor.numero %>"/>
                                                      </div>
                                                  </div>
                                              </div>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                                                          <input class="form-control" name="complement" type="text" placeholder="Complemento"  value="<%= investor.complemento %>"/>
                                                      </div>
                                                  </div>
                                              </div>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                                                          <input class="form-control city" name="city" type="text" placeholder="Cidade" value="<%= investor.cidade %>" />
                                                      </div>
                                                  </div>
                                              </div>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                                                          <input class="form-control state" name="state" type="text" placeholder="Estado"  value="<%= investor.estado %>"/>
                                                      </div>
                                                  </div>
                                              </div>
                                            </div>
                                            <div class="form-section">
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                                                          <input class="form-control" name="rg" type="file" placeholder="Anexo do RG" />
                                                      </div>
                                                  </div>
                                              </div>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                                                          <input class="form-control" name="rgverso" type="file" placeholder="RG (verso)" />
                                                      </div>
                                                  </div>
                                              </div>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                                                          <input class="form-control" name="residencia" type="file" placeholder="Comprovante de residência" />
                                                      </div>
                                                  </div>
                                              </div>
                                            </div>
                                            <div class="form-navigation">
                                              <div class="btn-form input-outer col-xs-12">
                                                  <div class="button-outer relative">
                                                      <button type="button" class="btn btn-fill previous">Voltar</button>
                                                      <button type="button" class="btn btn-fill next">Próximo</button>
                                                      <button type="submit" class="btn btn-fill submitbtn">Enviar dados</button>
                                                  </div>
                                              </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </div>
    </main>
</body>
</html>
