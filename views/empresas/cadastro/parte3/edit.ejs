<!DOCTYPE html>
<html lang="en-US" dir="ltr">

<head>
    <% include ../../../partials/includes %>
    <link rel="stylesheet" href="/stylesheets/cadastro-investidor.css">
    <script type="text/javascript" src="/scripts/multiStepForm.js"></script>
    <script type="text/javascript" src="/scripts/empresaForm.js"></script>
</head>

<body data-spy="scroll" data-target=".onpage-navigation" data-offset="60">
    <main>
        <% include ../../../partials/headerEmpreendedor %>
        <div class="registration-page">
            <section class="features-section-1 relative image-bg-5 full-screen-js">
                <div class="container">
                    <div class="row inner-wrapper">
                        <div class="col-xs-12">
                            <div class="template-logo text-center">
                                <a class="logo center img-responsive" href="#"><img src="/img/logo-light.png" alt="Main Logo" /></a>
                            </div>
                            <div class="section-header margin-t-30 margin-b-30 text-center light-text">
                                <h4 class="section-heading text-uppercase letter-spacing-4">dados financeiros</h4></div>
                        </div>
                        <div class="col-md-10 col-md-offset-1 col-sm-12">
                            <div class="row">
                                <div class="registration-form col-md-offset-3 col-md-6 col-sm-offset-2 col-sm-8 col-xs-12">
                                    <div class="registration-form-outer background-1">
                                        <% if(typeof errors !== 'undefined') { %>
                                        <% 		errors.forEach(function(error){ %>
                                              <div class="alert alert-danger"><%= error.msg %></div>
                                        <%		}); %>
                                        <% } %>
                                        <form class="single-form" method="post" action="/dashboard/cadastro-empresa/edit">
                                            <input type="hidden" name="parte" value="3">
                                            <div class="form-section current">
                                              <h6>Contador</h6>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-user"></i></div>
                                                          <input class="form-control" name="nomeContador" type="text" placeholder="Nome" value="<%= dadosFinanceiros.contatos.contador.nome %>"/>
                                                      </div>
                                                  </div>
                                              </div>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-user"></i></div>
                                                          <input class="form-control" name="emailContador" type="text" placeholder="E-mail" value="<%= dadosFinanceiros.contatos.contador.email %>"/>
                                                      </div>
                                                  </div>
                                              </div>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-user"></i></div>
                                                          <input class="form-control telefoneMask" name="telefoneContador" type="text" placeholder="Telefone" value="<%= dadosFinanceiros.contatos.contador.telefone %>"/>
                                                      </div>
                                                  </div>
                                              </div>
                                              <h6>Locador</h6>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-user"></i></div>
                                                          <input class="form-control" name="nomeLocador" type="text" placeholder="Nome" value="<%= dadosFinanceiros.contatos.aluguel.nome %>"/>
                                                      </div>
                                                  </div>
                                              </div>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-user"></i></div>
                                                          <input class="form-control" name="emailContador" type="text" placeholder="E-mail" value="<%= dadosFinanceiros.contatos.aluguel.email %>"/>
                                                      </div>
                                                  </div>
                                              </div>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-user"></i></div>
                                                          <input class="form-control telefoneMask" name="telefoneContador" type="text" placeholder="Telefone" value="<%= dadosFinanceiros.contatos.aluguel.telefone %>"/>
                                                      </div>
                                                  </div>
                                              </div>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-user"></i></div>
                                                          <input class="form-control moneyMask" name="valAluguel" type="text" placeholder="Valor do aluguel" value="<%= dadosFinanceiros.contatos.aluguel.valor %>"/>
                                                      </div>
                                                  </div>
                                              </div>
                                            </div>
                                            <div class="form-section">
                                              <h6>Fluxo</h6>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-user"></i></div>
                                                          <input class="form-control moneyMask" name="faturamento" type="text" placeholder="Faturamento mensal" value="<%= dadosFinanceiros.fluxo.faturamento %>"/>
                                                      </div>
                                                  </div>
                                              </div>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-user"></i></div>
                                                          <input class="form-control moneyMask" name="lucro" type="text" placeholder="Lucro mensal" value="<%= dadosFinanceiros.fluxo.lucro %>"/>
                                                      </div>
                                                  </div>
                                              </div>
                                            </div>
                                            <div class="form-section">
                                              <h6>Outros empréstimos</h6>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-user"></i></div>
                                                          <input class="form-control moneyMask" name="valorParcela" type="text" placeholder="Valor de cada parcela" value="<%= dadosFinanceiros.outrosEmprestimos.valorParcela %>"/>
                                                      </div>
                                                  </div>
                                              </div>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-user"></i></div>
                                                          <input class="form-control dataMask" name="dataParcela" type="text" placeholder="Data da última parcela" value="<%= dadosFinanceiros.outrosEmprestimos.dataParcela %>"/>
                                                      </div>
                                                  </div>
                                              </div>
                                              <div class="input-outer col-xs-12">
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                                                          <input class="form-control" name="taxaJuros" type="text" placeholder="Taxa de juros" value="<%= dadosFinanceiros.outrosEmprestimos.taxaJuros %>"/>
                                                      </div>
                                                  </div>
                                              </div>
                                            </div>
                                            <div class="form-section" id="dadosGastos">
                                              <h6>Gastos financeiros</h6>
                                              <div class="input-outer col-xs-12">
                                                  <p>Número de gastos</p>
                                                  <div class="select-outer">
                                                      <select class="form-control contact-plan" name="numeroGastos">
                                                        <option value="1" <% if(dadosFinanceiros.outrosEmprestimos.numeroGastos == 1){ %> selected <% } %>>1</option>
                            														<option value="2" <% if(dadosFinanceiros.outrosEmprestimos.numeroGastos == 2){ %> selected <% } %>>2</option>
                            														<option value="3" <% if(dadosFinanceiros.outrosEmprestimos.numeroGastos == 3){ %> selected <% } %>>3</option>
                            														<option value="4" <% if(dadosFinanceiros.outrosEmprestimos.numeroGastos == 4){ %> selected <% } %>>4</option>
                            														<option value="5" <% if(dadosFinanceiros.outrosEmprestimos.numeroGastos == 5){ %> selected <% } %>>5</option>
                                                      </select>
                                                  </div>
                                              </div>
                                              <% var i = 0;
                          										 	 for(i = 0; i < dadosFinanceiros.outrosGastos.numeroGastos; i++) { %>
                          												 <div class='input-outer col-xs-12 gasto'>
                                                    <div class="form-group">
                                                      <div class="input-group">
                                                        <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                          												 		  <input type='text' class="form-control" name='nomeSoc moneyMask' value="<%= pessoaJuridica.outrosGastos.valor[i] %>">
                                                      </div>
                                                    </div>
                          												 </div>
                                                   <div class='input-outer col-xs-12 gasto'>
                                                    <div class="form-group">
                                                      <div class="input-group">
                                                        <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                          												 		  <input type='text' class="form-control" name='emailSoc' value="<%= pessoaJuridica.outrosGastos.finalidade[i] %>">
                                                      </div>
                                                    </div>
                          												 </div>
                          										<% } %>
                                            </div>
                                            <div class="form-navigation">
                                              <div class="btn-form input-outer col-xs-12">
                                                  <div class="button-outer relative">
                                                      <button type="button" class="btn btn-fill previous">Voltar</button>
                                                      <button type="button" class="btn btn-fill next">Próximo</button>
                                                      <button type="submit" class="btn btn-fill submitbtn">Enviar dados</button>
                                                  </div>
                                              </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </div>
    </main>
</body>
</html>
