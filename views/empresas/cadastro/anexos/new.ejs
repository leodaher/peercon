<!DOCTYPE html>
<html lang="en-US" dir="ltr">

<head>
    <% include ../../../partials/includes %>
    <link rel="stylesheet" href="/stylesheets/cadastro-investidor.css">
    <script type="text/javascript" src="/scripts/multiStepForm.js"></script>
</head>

<body data-spy="scroll" data-target=".onpage-navigation" data-offset="60">
    <main>
        <% include ../../../partials/headerInvestidor %>
        <div class="registration-page">
            <section class="features-section-1 relative image-bg-5 full-screen-js">
                <div class="container">
                    <div class="row inner-wrapper">
                        <div class="col-xs-12">
                            <div class="template-logo text-center">
                                <a class="logo center img-responsive" href="#"><img src="/img/logo-light.png" alt="Main Logo" /></a>
                            </div>
                            <div class="section-header margin-t-30 margin-b-30 text-center light-text">
                                <h4 class="section-heading text-uppercase letter-spacing-4">anexos</h4></div>
                        </div>
                        <div class="col-md-10 col-md-offset-1 col-sm-12">
                            <div class="row">
                                <div class="registration-form col-md-offset-3 col-md-6 col-sm-offset-2 col-sm-8 col-xs-12">
                                    <div class="registration-form-outer background-1">
                                        <% if(typeof errors !== 'undefined') { %>
                                        <% 		errors.forEach(function(error){ %>
                                              <div class="alert alert-danger"><%= error.msg %></div>
                                        <%		}); %>
                                        <% } %>
                                        <form class="single-form" method="post" action="/dashboard/cadastro-empresa" enctype="multipart/form-data">
                                            <input type="hidden" name="parte" value="anexos">
                                            <div class="form-section current">
                                              <h6>Pessoa física</h6>
                                              <div class="input-outer col-xs-12">
                                                  <p>Anexo do RG</p>
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                                                          <input class="form-control" name="rg" type="file" placeholder="Anexo do RG" />
                                                      </div>
                                                  </div>
                                              </div>
                                              <div class="input-outer col-xs-12">
                                                  <p>Anexo do RG (verso)</p>
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                                                          <input class="form-control" name="rgverso" type="file" placeholder="RG (verso)" />
                                                      </div>
                                                  </div>
                                              </div>
                                              <div class="input-outer col-xs-12">
                                                  <p>Anexo do comprovante de residência</p>
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                                                          <input class="form-control" name="residencia" type="file" placeholder="Comprovante de residência" />
                                                      </div>
                                                  </div>
                                              </div>
                                              <h6>Empresa</h6>
                                              <div class="input-outer col-xs-12">
                                                  <p>Contrato social (completo)</p>
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                                                          <input class="form-control" name="contratoSocial" type="file" placeholder="Contrato social completo" />
                                                      </div>
                                                  </div>
                                              </div>
                                              <h6>Relacionamento com o banco</h6>
                                              <div class="input-outer col-xs-12">
                                                  <p>Extrato dos últimos 4 meses</p>
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                                                          <input class="form-control" name="extratoCompleto" type="file" placeholder="Extrato dos últimos 4 meses" />
                                                      </div>
                                                  </div>
                                                  <p>OU</p>
                                                  <p>Extrato de Dezembro</p>
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                                                          <input class="form-control" name="extrato1" type="file" placeholder="Extrato dos últimos 4 meses" />
                                                      </div>
                                                  </div>
                                                  <p>Extrato de Dezembro</p>
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                                                          <input class="form-control" name="extrato2" type="file" placeholder="Extrato dos últimos 4 meses" />
                                                      </div>
                                                  </div>
                                                  <p>Extrato de Dezembro</p>
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                                                          <input class="form-control" name="extrato3" type="file" placeholder="Extrato dos últimos 4 meses" />
                                                      </div>
                                                  </div>
                                                  <p>Extrato de Dezembro</p>
                                                  <div class="form-group">
                                                      <div class="input-group">
                                                          <div class="input-group-addon"><i class="fa fa-lock"></i></div>
                                                          <input class="form-control" name="extrato4" type="file" placeholder="Extrato dos últimos 4 meses" />
                                                      </div>
                                                  </div>
                                              </div>
                                            </div>
                                            <div class="form-navigation">
                                              <div class="btn-form input-outer col-xs-12">
                                                  <div class="button-outer relative">
                                                      <button type="button" class="btn btn-fill previous">Voltar</button>
                                                      <button type="button" class="btn btn-fill next">Próximo</button>
                                                      <button type="submit" class="btn btn-fill submitbtn">Enviar dados</button>
                                                  </div>
                                              </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        </div>
    </main>
</body>
</html>
